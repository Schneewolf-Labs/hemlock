import { random_bytes, generate_aes_key, generate_iv } from "@stdlib/crypto";

print("Testing random_bytes(32)");
let rb = random_bytes(32);
print("random_bytes result type: " + typeof(rb));
if (rb != null) {
    print("random_bytes result length: " + typeof(rb.length));
}

print("\nTesting generate_aes_key()");
let key = generate_aes_key();
print("generate_aes_key result type: " + typeof(key));
if (key != null) {
    print("key length: " + typeof(key.length));
}

print("\nTesting generate_iv()");
let iv = generate_iv();
print("generate_iv result type: " + typeof(iv));
if (iv != null) {
    print("iv length: " + typeof(iv.length));
}
