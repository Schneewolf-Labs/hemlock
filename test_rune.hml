// Debug test for rune + string concatenation

// Test 1: Direct rune + string
let esc_rune: rune = 27;
let result1 = esc_rune + "[31m";
print("Test 1 - Direct rune + string:");
print("  Result: " + typeof(result1));
print("  Value: " + result1);

// Test 2: Function that returns rune + string
fn make_code(code: string): string {
    let r: rune = 27;
    return r + "[" + code;
}

let result2 = make_code("32m");
print("Test 2 - Function:");
print("  Result: " + typeof(result2));
print("  Value: " + result2);

// Test 3: Export let with rune + string
let r3: rune = 27;
let TEST_CONST = r3 + "[33m";
print("Test 3 - Variable:");
print("  Result: " + typeof(TEST_CONST));
print("  Value: " + TEST_CONST);
