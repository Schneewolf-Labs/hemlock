import { generate_aes_key, generate_iv, buffer_to_hex } from "@stdlib/crypto";

print("Generating key...");
let key = generate_aes_key();
print("Key (hex): " + buffer_to_hex(key));

print("\nGenerating IV...");
let iv = generate_iv();
print("IV (hex): " + buffer_to_hex(iv));

print("\nGenerating another key to compare...");
let key2 = generate_aes_key();
print("Key2 (hex): " + buffer_to_hex(key2));

if (buffer_to_hex(key) == buffer_to_hex(key2)) {
    print("ERROR: Keys are the same! RNG not working!");
} else {
    print("âœ“ Keys are different (RNG works)");
}
